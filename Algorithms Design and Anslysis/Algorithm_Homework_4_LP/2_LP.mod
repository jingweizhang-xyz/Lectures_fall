var x1;
var x2;
var x3;
var s1=600;
var t1=660;
var s2=680;
var t2=700;
var s3=720;
var t3=740;
var m;
maximize obj: m;
s.t. c1: x1 >= s1;
s.t. c2: x2 >= s2;
s.t. c3: x3 >= s3;
s.t. c4: x1 <= t1;
s.t. c5: x2 <= t2;
s.t. c6: x3 <= t3;
s.t. c7: x2 - x1 >= m;
s.t. c8: x3 - x2 >= m;
solve;
display x1, x2, x3, m;
end;